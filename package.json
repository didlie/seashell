    "body-parser": "^1.17.1",
    "chalk": "^1.1.3",
    "express": "^4.15.2",
    "fs-promise": "^2.0.2",    
    "isomorphic-ws": "^2.3.1",
    "morgan": "^1.8.1",
    "path-to-regexp": "^1.7.0",
    "prettyjson": "^1.2.1",
    "shelljs": "^0.7.7",
    "uuid": "~2.0.2",
    "webpack": "^2.4.1",
    "webpack-node-externals": "^1.5.4",
    "ws": "^2.3.1",
    "yargs": "^7.1.0"
